{
  "manifest_version": 2,

  "name": "Target Calender Exporter",
  "description": "This extension grabs the calendar data from Target's employee selfservice website and puts in in Google Calendar.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://wss.target.com/selfservice/home/viewSchedule.do/*"],
      // "matches": ["file:///D:/github/jgollhardt/My%20Schedule.html"],
      "js": ["fetch_data.js"]
    }
  ]
}